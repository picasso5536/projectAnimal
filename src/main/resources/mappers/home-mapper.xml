<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="home">
	
	<!-- 아이디 중복검사 -->
<select id="id" parameterType="mvo" resultType="Integer">
    SELECT COUNT(*) FROM MEMBERS WHERE mbr_id = #{mbr_id}
</select>

	<!-- 닉네임 중복검사 -->
<select id="nickname" parameterType="mvo" resultType="Integer">
    SELECT COUNT(*) FROM MEMBERS WHERE mbr_nickname = #{mbr_nickname}
</select>
	
	<!-- 로그인 -->


</mapper>